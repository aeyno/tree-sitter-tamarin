================================================================================
More complex tactic
================================================================================


theory ComplexTactic
begin

tactic: superTactic
presort: C
prio:
	regex "~~>"
prio:
  regex "AgSt"
deprio:
  regex "*aaa!!ùùùµ%%%$$$$$''çççàà^^^"

tactic: anotherTacticcc
presort: C
prio:  
  regex "!KU\( ~skS"
prio:
  regex "In_S"
prio:
  regex "In_A"
prio:
  regex "!KU\( ~r1"
prio:
  regex "!KU\( ~rb1"
prio:
  regex "!KU\( ~skS"
prio:
  regex "!KU\( h\("
prio:
  regex "~~>"
prio:
  regex "AgSt"
prio:
  regex "In_S"
prio:
  regex "In_A"
prio:
  regex "cp\("

end

